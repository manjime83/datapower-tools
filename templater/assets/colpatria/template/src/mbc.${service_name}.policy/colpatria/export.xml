<datapower-configuration version="3">
	<export-details>
		<user>AOS</user>
		<domain>BVirtual</domain>
		<comment>$Id: export.xml 121 2017-07-04 17:13:23Z manuel.jimenez $</comment>
		<device-name>IDG</device-name>
		<firmware-version>IDG.7.5.1.0</firmware-version>
		<current-date>2017-06-20</current-date>
	</export-details>
	<configuration domain="colpatria">
		<StylePolicyAction name="mbc.${service_name}.request_xformbin_0">
			<mAdminState>enabled</mAdminState>
			<Type>xformbin</Type>
			<Input>INPUT</Input>
			<TransformLanguage>none</TransformLanguage>
			<TxMap>local:///services/mbc.${service_name}/dpa/XML_${service_code}.dpa</TxMap>
			<ActionDebug persisted="false">off</ActionDebug>
			<TxMode>dpa</TxMode>
			<Output>OUTPUT</Output>
			<NamedInOutLocationType>default</NamedInOutLocationType>
			<SSLClientConfigType>proxy</SSLClientConfigType>
			<OutputType>default</OutputType>
			<Transactional>off</Transactional>
			<SOAPValidation>body</SOAPValidation>
			<SQLSourceType>static</SQLSourceType>
			<JWSVerifyStripSignature>on</JWSVerifyStripSignature>
			<Asynchronous>off</Asynchronous>
			<ResultsMode>first-available</ResultsMode>
			<RetryCount>0</RetryCount>
			<RetryInterval>1000</RetryInterval>
			<MultipleOutputs>off</MultipleOutputs>
			<IteratorType>XPATH</IteratorType>
			<Timeout>0</Timeout>
			<MethodRewriteType>GET</MethodRewriteType>
			<MethodType>POST</MethodType>
			<MethodType2>POST</MethodType2>
		</StylePolicyAction>
		<StylePolicyRule name="mbc.${service_name}.request">
			<mAdminState>enabled</mAdminState>
			<Direction>request-rule</Direction>
			<InputFormat>none</InputFormat>
			<OutputFormat>none</OutputFormat>
			<NonXMLProcessing>off</NonXMLProcessing>
			<Unprocessed>off</Unprocessed>
			<Actions class="StylePolicyAction">mbc.${service_name}.request_xformbin_0</Actions>
		</StylePolicyRule>
		<StylePolicyAction name="mbc.${service_name}.response_xformbin_0">
			<mAdminState>enabled</mAdminState>
			<Type>xformbin</Type>
			<Input>INPUT</Input>
			<TransformLanguage>none</TransformLanguage>
			<TxMap>local:///services/mbc.${service_name}/dpa/${service_code}_XML.dpa</TxMap>
			<ActionDebug persisted="false">off</ActionDebug>
			<TxMode>dpa</TxMode>
			<Output>OUTPUT</Output>
			<NamedInOutLocationType>default</NamedInOutLocationType>
			<SSLClientConfigType>proxy</SSLClientConfigType>
			<OutputType>default</OutputType>
			<Transactional>off</Transactional>
			<SOAPValidation>body</SOAPValidation>
			<SQLSourceType>static</SQLSourceType>
			<JWSVerifyStripSignature>on</JWSVerifyStripSignature>
			<Asynchronous>off</Asynchronous>
			<ResultsMode>first-available</ResultsMode>
			<RetryCount>0</RetryCount>
			<RetryInterval>1000</RetryInterval>
			<MultipleOutputs>off</MultipleOutputs>
			<IteratorType>XPATH</IteratorType>
			<Timeout>0</Timeout>
			<MethodRewriteType>GET</MethodRewriteType>
			<MethodType>POST</MethodType>
			<MethodType2>POST</MethodType2>
		</StylePolicyAction>
		<StylePolicyRule name="mbc.${service_name}.response">
			<mAdminState>enabled</mAdminState>
			<Direction>response-rule</Direction>
			<InputFormat>none</InputFormat>
			<OutputFormat>none</OutputFormat>
			<NonXMLProcessing>off</NonXMLProcessing>
			<Unprocessed>off</Unprocessed>
			<Actions class="StylePolicyAction">mbc.${service_name}.response_xformbin_0</Actions>
		</StylePolicyRule>
		<StylePolicy name="mbc.${service_name}">
			<mAdminState>enabled</mAdminState>
			<DefStylesheetForSoap>store:///filter-reject-all.xsl</DefStylesheetForSoap>
			<DefStylesheetForXsl>store:///identity.xsl</DefStylesheetForXsl>
			<DefXQueryForJSON>store:///reject-all-json.xq</DefXQueryForJSON>
			<PolicyMaps>
				<Match class="Matching">__default-accept-service-providers__</Match>
				<Rule class="StylePolicyRule">mbc.${service_name}.request</Rule>
			</PolicyMaps>
			<PolicyMaps>
				<Match class="Matching">__default-accept-service-providers__</Match>
				<Rule class="StylePolicyRule">mbc.${service_name}.response</Rule>
			</PolicyMaps>
		</StylePolicy>
	</configuration>
	<files>
		<file name="local:///services/mbc.${service_name}/dpa/XML_${service_code}.dpa" src="local/services/mbc.${service_name}/dpa/XML_${service_code}.dpa" />
		<file name="local:///services/mbc.${service_name}/dpa/${service_code}_XML.dpa" src="local/services/mbc.${service_name}/dpa/${service_code}_XML.dpa" />
	</files>
</datapower-configuration>